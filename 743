#include <stack>
#include <string>
#include <iostream>
using std::cin;
using std::cout;
using std::stack;
using std::string;
int find_unmatched(string s) {
	stack<char> visited_brackets;
	if (s.empty()) {
		return -1;
	}
	for (int i = 0; i != s.length; ++i) {
		switch (s.at(i))
		{
			case '(':case'[':case'{':
				visited_brackets.push(s.at(i));
				break;
			case ')':case']':case'}': 
				char paired_bracket = s.at(i) - 1 - (s.at(i) != '(');
				char current = '\0';
				string temp = "";
				while (!visited_brackets.empty() && current != paired_bracket) {
					current = visited_brackets.top();
					visited_brackets.pop();
				}

				break;

			default:
				break;
		}
	}
}
int main() {
	string text;
	cin >> text;

	return 0;
}
